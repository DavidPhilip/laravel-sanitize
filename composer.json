{
	"name" : "webpatser/laravel-sanitize",
	"description" : "Laravel package to sanatize a string for use as URL's. It uses the sanitize code midgard/midgardmvc-helper-urlize.",
	"version" : "1.0",
	"type" : "laravel-library",
	"authors" : [{
			"name" : "Christoph Kempen",
			"email" : "christoph@downsized.nl"
		}
	],
	"keywords" : [
		"laravel sanitize URL"
	],
	"homepage" : "https://github.com/webpatser/laravel-sanitize",
	"license" : [
		"MIT"
	],
	"require" : {
		"php" : ">=5.3.0",
        "midgard/midgardmvc-helper-urlize": "dev-master"
	},
	"autoload" : {
		"psr-0" : {
			"Webpatser\\Sanitize" : "src/"
		}
	},
	"minimum-stability" : "dev",
	"support" : {
		"source" : "https://github.com/webpatser/laravel-sanitize",
		"issues" : "https://github.com/webpatser/laravel-sanitize/issues"
	}
}